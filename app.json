{
  "expo": {
    "name": "OneTapExpense",
    "slug": "OneTapExpense",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "onetapexpense",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
